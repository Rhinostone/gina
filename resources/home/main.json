{
    "frameworks": {
        "{release}": [ "{version}" ]
    },
    "def_framework": "{version}",
    "envs": {
        "{release}": [ "dev", "prod" ]
    },
    "def_env": {
      "{release}": "dev"
    },
    "dev_env": {
        "{release}": "dev"
    },
    
    "protocols": {
        "_comment": "e.g.:  http/1.1, http/2.0, net",
        "{release}": [  "http/1.1", "http/2.0" ]
    },
    "def_protocol": {
        "{release}": "http/1.1"
    },
    "schemes": {
        "_comment": "e.g.:  http, https",
        "{release}": [  "http", "https" ]
    },
    "def_scheme": {
        "{release}": "http"
    },
    "cultures": {
        "_comment": "e.g.:  en-US, fr-FR ... available for the framework then used to translate the admin dashboard",
        "{release}": [  "en-US", "fr-FR" ]
    },
    "def_culture": {
        "{release}": "en-US"
    },
    "def_timezone": {
        "{release}": "Africa/Douala"
    },
    "log_levels": {
        "_comment": "hierarchy",
        "{release}": [  "trace", "debug", "info", "warn", "error", "fatal" ]
    },
    "def_log_level": {
        "{release}": "info"
    }
}