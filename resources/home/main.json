{
    "frameworks": {
        "{release}": [ "{version}" ]
    },
    "def_framework": "{version}",
    "def_prefix": {
        "{release}": "{prefix}"
    },
    "def_global_mode": {
        "{release}": "{global_mode}"
    },
    "archs": {
        "{release}": [
            "arm",
            "arm64",
            "x64",
            "amd64"
        ]
    },
    "def_arch": {
        "{release}": "x64"
    },
    "platforms": {
        "{release}": [
            "darwin",
            "linux",
            "win32",
            "freebsd",
            "openbsd",
            "sunos",
            "aix"
        ]
    },
    "def_platform": {
        "{release}": "linux"
    },
    "scopes": {
        "{release}": [
            "local",
            "production"
        ]
    },
    "def_scope": {
        "{release}": "local"
    },
    "local_scope": {
        "{release}": "local"
    },
    "production_scope": {
        "{release}": "production"
    },
    "envs": {
        "{release}": [ "dev", "prod" ]
    },
    "def_env": {
      "{release}": "dev"
    },
    "dev_env": {
        "{release}": "dev"
    },

    "protocols": {
        "_comment": "e.g.:  http/1.1, http/2.0, net",
        "{release}": [  "http/1.1", "http/2.0" ]
    },
    "def_protocol": {
        "{release}": "http/1.1"
    },
    "schemes": {
        "_comment": "e.g.:  http, https",
        "{release}": [  "http", "https" ]
    },
    "def_scheme": {
        "{release}": "http"
    },
    "cultures": {
        "_comment": "e.g.:  en_US, fr_FR etc. available for the framework then used to translate the admin dashboard",
        "{release}": [  "en_CM", "fr_CM" ]
    },
    "def_culture": {
        "{release}": "en_CM"
    },
    "def_iso_short": {
        "{release}": "cm"
    },
    "def_timezone": {
        "{release}": "Africa/Douala"
    },
    "def_date": {
        "{release}": "yyyy/mm/dd"
    },
    "log_levels": {
        "_comment": "hierarchy",
        "{release}": [  "trace", "debug", "info", "warn", "error", "fatal" ]
    },
    "def_log_level": {
        "{release}": "info"
    }
}